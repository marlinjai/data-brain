{
  "name": "@marlinjai/data-brain-sdk",
  "version": "0.2.0",
  "description": "TypeScript SDK for Data Brain â€” structured data as a service",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marlinjai/data-brain",
    "directory": "packages/sdk"
  },
  "keywords": ["database", "data", "cloudflare", "d1", "sdk", "typescript", "api"],
  "author": "marlinjai",
  "license": "MIT",
  "dependencies": {
    "@marlinjai/data-table-core": "^0.1.0"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
